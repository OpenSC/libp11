# Load common test functions
. ${srcdir}/rsa-common.sh

# Do the common test initialization
common_init

../examples/auth ${MODULE} ${PIN}
if test $? != 0;then
	echo "Basic PKCS #11 test test failed"
	exit 1;
fi

